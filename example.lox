//calculates the nth term of the fibonacci sequence

var n = 10;
var fib1 = 1;
var fib2 = 1;
var soma = 0;

var count = 3;

while(count <= n){
    soma = fib1 + fib2;
    fib1 = fib2;
    fib2 = soma;
    count = count + 1;
}

/*
the above while statement is equivalent to:
for(var count = 3; count <= n; count = count + 1){
    soma = fib1 + fib2;
    fib1 = fib2;
    fib2 = soma;
}
*/

